{
  "name": "@jkavuncuoglu/vue-softphone",
  "version": "1.0.0",
  "description": "A provider independent Vue.js 2 softphone component",
  "main": "dist/vue-softphone.js",
  "module": "dist/vue-softphone.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "vue": "^2.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^27.0.0",
    "jest": "^27.0.0",
    "postcss": "^8.4.27",
    "rollup": "^2.60.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "^3.3.3",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.7.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/index.js"
    ],
    "testEnvironment": "jsdom"
  },
  "keywords": [
    "vue",
    "vue2",
    "softphone",
    "amazon connect",
    "twilio",
    "8x8",
    "ring central",
    "telephony"
  ],
  "author": "Jeremy Kavuncuoglu <jkavuncuoglu@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkavuncuoglu/vue-softphone.git"
  },
  "bugs": {
    "url": "https://github.com/jkavuncuoglu/vue-softphone/issues"
  },
  "homepage": "https://github.com/jkavuncuoglu/vue-softphone#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
